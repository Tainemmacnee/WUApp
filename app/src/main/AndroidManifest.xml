<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wuapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.wuapp.ui.activity.MainActivity"
            android:parentActivityName="com.example.wuapp.ui.activity.LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.wuapp.ui.activity.LoginActivity" />
        </activity>
        <activity android:name="com.example.wuapp.ui.activity.ReportResultActivity"
            android:parentActivityName="com.example.wuapp.ui.activity.MainActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.wuapp.ui.activity.MainActivity" />
        </activity>
        <activity android:name="com.example.wuapp.ui.activity.DisplayEventScoresActivity"
            android:parentActivityName="com.example.wuapp.ui.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.wuapp.ui.activity.MainActivity" />
        </activity>
        <activity android:name="com.example.wuapp.ui.activity.DisplayEventTeamsActivity"
            android:parentActivityName="com.example.wuapp.ui.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.wuapp.ui.activity.MainActivity" />
        </activity>
        <activity android:name="com.example.wuapp.ui.activity.DisplayMapActivity"
            android:parentActivityName="com.example.wuapp.ui.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.wuapp.ui.activity.MainActivity" />
        </activity>
        <activity android:name="com.example.wuapp.ui.activity.DisplayGameActivity"
            android:parentActivityName="com.example.wuapp.ui.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.wuapp.ui.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.example.wuapp.ui.activity.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>