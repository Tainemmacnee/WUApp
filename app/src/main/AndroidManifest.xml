<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Wuapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.Wuapp.DisplayEventTeamsActivity"
            android:parentActivityName="com.example.Wuapp.DisplayUserActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.Wuapp.DisplayUserActivity" />
        </activity>
        <activity android:name="com.example.Wuapp.DisplayUserActivity" />
        <activity android:name="com.example.Wuapp.ReportResultActivity"
            android:parentActivityName="com.example.Wuapp.DisplayUserActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.Wuapp.DisplayUserActivity" />
        </activity>
        <activity android:name="com.example.Wuapp.DisplayEventStandingsActivity"
            android:parentActivityName="com.example.Wuapp.DisplayUserActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.Wuapp.DisplayUserActivity" />
        </activity>
        <activity
            android:name="com.example.Wuapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>